<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.jacky.launcher">

    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.GET_TASKS"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.SET_WALLPAPER"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.EXPAND_STATUS_BAR"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES"/>
    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:name="com.jacky.compatible.launcher.LauncherApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:theme="@android:style/Theme.Holo.Light.NoActionBar.Fullscreen">

        <activity android:name="com.jacky.compatible.launcher.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>

        <activity android:name="com.jacky.compatible.launcher.features.Ethernet"/>
        <activity android:name="com.jacky.compatible.launcher.features.Bluetooth"/>
        <activity android:name="com.jacky.compatible.launcher.features.setting.SettingCustom"/>
        <activity android:name="com.jacky.compatible.launcher.features.app.AppUninstall"/>
        <activity android:name="com.jacky.compatible.launcher.features.app.AppAutoRun"/>
        <activity android:name="com.jacky.compatible.launcher.features.speedtest.SpeedTestActivity"/>
        <activity
            android:name="com.jacky.compatible.launcher.features.eliminateprocess.EliminateMainActivity"
            android:theme="@android:style/Theme.Dialog"/>
        <activity
            android:name="com.jacky.compatible.launcher.features.garbageclear.GarbageClear"
            android:theme="@android:style/Theme.Dialog"/>
        <activity android:name="com.jacky.compatible.launcher.features.wifi.WifiActivity"/>
    </application>

</manifest>